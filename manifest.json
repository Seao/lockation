{
  "name": "__MSG_extname__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Keep your location private",
  "homepage_url": "https://github.com/Seao/lockation",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "permissions": [
    "geolocation"
  ]
}
